node {
  stage ('======== Clone repository ========') {
    checkout scm
  }
  stage ('======== Build image ========') {
    app = docker.build("edu/ubuntu")
  }
  stage ('======== Push image ========') {
    docker.withRegistry('https://223.130.133.184', 'harbor_ci') {
      app.push("${env.BUILD_NUMBER}")
      app.push("latest")
    }
  }
}
