node {
  stage ('======== Clone repository ========') {
    checkout scm
  }
  stage ('======== Build image ========') {
    app = docker.build("test/nginx")
  }
  stage ('======== Push image ========') {
    docker.withRegistry('https://myharbor.apps.211-34-231-93.nip.io', 'harbor_ci') {
      app.push("${env.BUILD_NUMBER}")
      app.push("latest")
    }
  }
}
